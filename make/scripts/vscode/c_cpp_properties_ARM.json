{
    "version": 4,
    "env": {
        "mtbCompilerArgs": [
            "&&_MTB_RECIPE__C_FLAGS&&"
        ],
        "mtbDefaultIncludePath": [
            &&_MTB_RECIPE__INCLUDE_LIST&&
            "&&_MTB_RECIPE__TOOL_CHAIN_DIRECTORY&&/include/**"
        ],
        "mtbDefaultDefines": [
            &&_MTB_RECIPE__DEFINE_LIST&&
            "__GNUC__"
        ]
    },
    "configurations": [
        {
            "name": "MTB",
            "compilerPath": "&&_MTB_RECIPE__TOOL_CHAIN_DIRECTORY&&/bin/armclang",
            "compilerArgs": [
                "${mtbCompilerArgs}"
            ],
            "compileCommands": "&&_MTB_CORE__VSCODE_CDB_FILE&&",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
                "${mtbDefaultIncludePath}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "${mtbDefaultDefines}"
            ]
        }
    ]
}
