{
    "version": 4,
    "env": {
        "mtbCompilerArgs": [
            "&&_MTB_RECIPE__C_FLAGS&&"
        ],
        "mtbDefaultIncludePath": [
            &&_MTB_RECIPE__INCLUDE_LIST&&
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/lib/gcc/arm-none-eabi/&&_MTB_RECIPE__GCC_VERSION&&/include",
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/lib/gcc/arm-none-eabi/&&_MTB_RECIPE__GCC_VERSION&&/include-fixed",
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/arm-none-eabi/include",
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/arm-none-eabi/include/c++/&&_MTB_RECIPE__GCC_VERSION&&",
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/arm-none-eabi/include/c++/&&_MTB_RECIPE__GCC_VERSION&&&&_MTB_CORE__VSCODE_CPU&&&&_MTB_CORE__VSCODE_VFP&&",
            "&&_MTB_RECIPE__GCC_DIRECTORY&&/arm-none-eabi/include/c++/&&_MTB_RECIPE__GCC_VERSION&&/backward"
        ],
        "mtbDefaultDefines": [
            &&_MTB_RECIPE__DEFINE_LIST&&
            "__GNUC__"
        ]
    },
    "configurations": [
        {
            "name": "MTB",
            "compilerPath": "&&_MTB_RECIPE__GCC_DIRECTORY&&/bin/arm-none-eabi-gcc",
            "compilerArgs": [
                "${mtbCompilerArgs}"
            ],
            "compileCommands": "&&_MTB_CORE__VSCODE_CDB_FILE&&",
            "intelliSenseMode": "gcc-arm",
            "cStandard": "c99",
            "cppStandard": "c++11",
            "includePath": [
                "${mtbDefaultIncludePath}"
            ],
            "browse": {
                "limitSymbolsToIncludedHeaders": true,
                "databaseFilename": ""
            },
            "defines": [
                "${mtbDefaultDefines}"
            ]
        }
    ]
}
